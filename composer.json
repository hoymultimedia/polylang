{
    "name": "wordplate/translator",
    "description": "A translations helper for WordPlate",
    "keywords": ["wordplate", "translator", "wordpress"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        },
        {
            "name": "Chris Andersson",
            "email": "hello@puredazzle.se"
        }
    ],
    "require": {
        "php": "^5.6.4 || ^7.0",
        "wpackagist-plugin/polylang": "^1.9"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ]
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "config": {
        "preferred-install": "dist"
    },
    "extra":{
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
