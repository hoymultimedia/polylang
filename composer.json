{
    "name": "wordplate/polylang",
    "description": "A Polylang helper for WordPlate",
    "keywords": ["wordplate", "polylang", "wordpress"],
    "license": "MIT",
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com",
            "homepage": "https://vinkla.com"
        },
        {
            "name": "Chris Andersson",
            "email": "hello@puredazzle.se",
            "homepage": "https://puredazzle.se"
        }
    ],
    "require": {
        "php": "^7.0",
        "wpackagist-plugin/polylang": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "WordPlate\\Tests\\Polylang\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "config": {
        "preferred-install": "dist"
    },
    "extra":{
        "branch-alias": {
            "dev-master": "4.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
